Cell[TextData[{
 "The binomial coefficients describe the number of ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b5335458-85de-470c-901a-cc652cd76ffc"],
 "-element subsets of a set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"1", ",", "2", ",", "...", ",", "n"}], "}"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ac8ebde8-d313-4850-97a4-5f4fdecbfd85"],
 " (\[OpenCurlyDoubleQuote]",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b44b37e2-8df9-4e90-9a3b-96af14226319"],
 " choose ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a0d93221-bbb3-4935-b771-8156a939476b"],
 "\[CloseCurlyDoubleQuote]).\n",
 StyleBox["Theorem 1.5: ",
  FontWeight->"Bold"],
 "The number of ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f5d76033-691a-469b-af09-0ec54427656e"],
 "-element subsets of a set with cardinality ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "befe81f7-1e2d-4acd-bffb-60c37f01a2bf"],
 " is equal to ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         RowBox[{"(", 
           GridBox[{{
              StyleBox["n", "TI"]}, {
              StyleBox["k", "TI"]}}], ")"}], "\[LongEqual]", 
         FractionBox[
          RowBox[{
            StyleBox["n", "TI"], "!"}], 
          RowBox[{
            StyleBox["k", "TI"], "!", 
            RowBox[{"(", 
              RowBox[{
                StyleBox["n", "TI"], "-", 
                StyleBox["k", "TI"]}], ")"}], "!"}]], "\[LongEqual]", 
         FractionBox[
          RowBox[{
            StyleBox["n", "TI"], 
            RowBox[{"(", 
              RowBox[{
                StyleBox["n", "TI"], "-", "1"}], ")"}], "\[CenterEllipsis]", 
            RowBox[{"(", 
              RowBox[{
                StyleBox["n", "TI"], "-", 
                StyleBox["k", "TI"], "+", "1"}], ")"}]}], 
          RowBox[{
            StyleBox["k", "TI"], "!"}]]}], TraditionalForm], "errors" -> {}, 
     "input" -> 
     "{n \\choose k}=\\frac{n!}{k!(n-k)!}=\\frac{n(n-1)\\cdots (n-k+1)}{k!}", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "4333be97-bd19-4f78-ae9e-c70cb35c0f7a"],
 ".\n"
}], "Text",
